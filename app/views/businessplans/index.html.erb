<h1>Listing businessplans</h1>

<table>
  <tr>
    <th>Wizard question</th>
    <th>Wizard answer</th>
    <th>Status</th>
    <th>Order</th>
    <th>Wizard step</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @businessplans.each do |businessplan| %>
  <tr>
    <td><%= businessplan.wizard_question %></td>
    <td><%= businessplan.wizard_answer %></td>
    <td><%= businessplan.status %></td>
    <td><%= businessplan.order %></td>
    <td><%= businessplan.wizard_step_id %></td>
    <td><%= link_to 'Show', businessplan %></td>
    <td><%= link_to 'Edit', edit_businessplan_path(businessplan) %></td>
    <td><%= link_to 'Destroy', businessplan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Businessplan', new_businessplan_path %>
